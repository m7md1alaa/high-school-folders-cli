{
  "name": "school-folders-creator",
  "version": "1.0.0",
  "main": "build/index.js",
  "type": "module",
  "bin": {
    "school-folders-creator": "./build/index.js"
  },
  "scripts": {
    "build": "ncc build index.js -o build",
    "package": "pkg . -t node16-macos-arm64,node16-macos-x64,node16-win-x64 --output dist/school-folders-creator"
  },
  "dependencies": {
    "@inquirer/prompts": "^5.3.8",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.1",
    "pkg": "^5.8.1"
  },
  "pkg": {
    "targets": [
      "node16-macos-arm64",
      "node16-macos-x64",
      "node16-win-x64"
    ],
    "outputPath": "dist"
  },
  "keywords": [],
  "author": "Mohammed alaa",
  "license": "ISC",
  "description": "CLI tool for creating organized folder structures for students",
  "engines": {
    "node": ">=16"
  }
}
